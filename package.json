{
  "name": "@sellis/lerna-monorepo",
  "version": "1.0.1",
  "private": true,
  "description": "Sample monorepo for react components",
  "main": "index.js",
  "author": "Scott Ellis <j.scott.ellis64@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "scripts": {
    "setup-dev": "yarn && yarn bootstrap && yarn build",
    "bootstrap": "lerna bootstrap",
    "publish-all": "lerna publish --registry http://localhost:4873/",
    "build": "lerna run build",
    "yalc:push": "lerna run yalc:push",
    "yalc:push:watch": "lerna run --parallel yalc:push:watch",
    "build:watch": "lerna run --parallel build:watch",
    "dev:watch": "run-p build:watch yalc:push:watch",
    "storybook:watch": "run-p storybook build:watch",
    "lint": "lerna run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build",
    "postinstall": "yarn build",
    "update": "yarn upgrade --latest",
    "create-component": "HYGEN_TMPLS=./.generator/_templates HYGEN_OVERWRITE=1 hygen react-mui-widget with-prompt"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "devDependencies": {
    "@apollo/client": "^3.3.13",
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-controls": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/core-common": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.21",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/storybook-react-router": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
