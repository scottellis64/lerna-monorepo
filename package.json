    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-devtools-extension": "^2.13.9"
  }
